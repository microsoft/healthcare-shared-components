{
  "version": "2.0",
  "Extensions": {
    "DurableTask": {
      "HubName": "WorkerIntegrationTests"
    }
  },
  "Logging": {
    "FileLoggingMode": "never",
    "LogLevel": {
      "default": "Information",
      "DurableTask": "Warning",
      "Function": "Warning",
      "Function.*.User": "Information",
      "Host": "Warning"
    }
  },
  "PurgeHistory": {
    "Frequency": "0 0 * * *"
  }
}
